@charset "utf-8";
.product{padding:10px}
.search{margin-bottom:24px;height:50px;background-color:#fff;position:relative}
.search .icon-search{position:absolute;top:0;left:0;z-index:2;width:55px;line-height:50px;font-size:24px;color:#c0c0c0;text-align:center}
.search .ui-input-search{position:absolute;left:55px;right:71px;top:0;height:50px;padding:0}
.search .ui-input-search input.ui-input-text{width:100%;padding:15px 0}
.search a.search_btn{position:absolute;width:70px;height:50px;line-height:50px;font-size:120%;right:0;top:0;z-index:2;text-align:center;border-left:1px dotted #d1d1d1;color:#000;text-decoration:none}
.product_list{padding:0 15px 15px}
.product_list li{background-color:#fff; border-bottom:1px dotted #c9c9c9}
.product_list .ui-li-thumb{top:22px}
.product_list h5{font-size:80%}
/*热门搜索*/
.search_hot{overflow:hidden}
.search_hot h2{padding-left:2%;line-height:200%;font-size:140%}
.search_hot li{float:left;width:29%;height:45px;margin:0 2%}
.search_hot li a{display:block;height:35px;line-height:35px;text-align:center;background-color:#fff}
.search_hot li a:active{background-color:#f4fdd7;color:#9dc814}
/*新闻*/
.news_list,.news_detail{padding:15px}
.news_list li{margin-bottom:10px;background-color:#fff;line-height:150%}
.news_list .ui-li .ui-btn-inner a.ui-link-inherit{padding:0 30px 0 15px}
.news_list h4{font-size:90%}
.news_list h4 span{padding-left:20px}
.news_list p{padding-left:20px;font-size:80%}
.news_detail{line-height:150%;overflow:auto}
.news_dtitle{margin-bottom:10px;font-size:100%;}
.news_dcontent p{margin-bottom:8px;font-size:80%}
.news_dtime{text-align:right;font-size:80%;color:#9dc814}
/*产品详情*/
.product_dpic{margin-bottom:10px;height:190px;text-align:center}
.product_dpic img{width:250px;height:188px;margin:0 auto}
.product_dinfo{padding-bottom:10px;margin-botton:10px;line-height:150%;border-bottom:1px solid #c9c9c9}
.product_dinfo h2{margin-bottom:6px;font-size:120%}
.product_dinfo p{color:#a6a6a6}
.product_dselect{margin-bottom:10px;padding:10px 0;border-top:1px solid #c9c9c9;border-bottom:1px solid #c9c9c9}
.product_dprice{overflow:hidden}
.product_dprice dl{padding:10px 0;margin-top:-1px;font-size:100%;border-top:1px dotted #c9c9c9}
.product_dprice dt{width:75px;float:left;text-align:right}
.product_dprice dd{margin-left:80px}
.product_dlist li{height:40px;overflow:hidden;border-top:1px solid #c9c9c9}
.product_dlist li a{display:block;padding:0 10px;line-height:40px;font-size:100%}
.product_dlist li em{float:right}
.product_dlist li a:active{color:#74afff}
.product_dcolor,.product_dnum{margin-bottom:8px;font-size:100%}
.product_dcolor dt,.product_dnum dt{width:65px;padding-right:8px;float:left;text-align:right;line-height:30px}
.product_dcolor dd{margin-left:75px;}
.product_dnum dd{float:left}
.product_dcolor dd a{float:left;height:30px;line-height:30px;padding:0 20px 0 10px}
.product_dcolor dd a.cur{border:1px solid #74afff;background:url(../images/main/select.gif) no-repeat right bottom}
.dplus a,.dminus a{display:block;border:1px solid #74afff;width:30px;height:30px;background-image:url(../images/main/pp.gif); background-repeat:no-repeat;overflow:hidden;text-indent:-1000px}
.dplus a{background-position:center 5px}
.dminus a{background-position:center -43px}
.dnum,.dminus{margin-left:-1px}
.dnum{height:30px;border:1px solid #74afff}
.dnum input{width:60px;padding:5px 0;text-align:center;font-size:16px;background:none;border:0}
.product_dbtn dt{margin-bottom:6px}
.product_dbtn dd{width:49%}
.product_dsimilar{margin-top:10px;border-top:1px solid #C9C9C9;padding-top:10px}
.product_dsimilar h2{margin-bottom:5px;line-height:140%;font-size:100%}
.product_dsimilar li{float:left;width:30%;margin:0 1%;padding:6px 0;background-color:#fff;text-align:center}
.product_dsimilar li img{width:80px;height:60px;}
.product_dsimilar li span{display:block;padding:0 6px;height:32px;line-height:16px;font-size:12px;overflow:hidden;white-space:normal;text-overflow:ellipsis}
.product_detail_tab{border-bottom:3px solid #74AFFF;}
.product_detail_tab li{cursor:pointer;float:left;font-weight:700;line-height:33px;padding:0 25px;}
.product_detail_tab li.cur{background:none repeat scroll 0 0 #74AFFF;color: #FFFFFF;}
.product_detail_info{overflow:auto}
.product_detail_table{margin:10px 0;border-bottom:1px solid #E3E3E3; line-height:30px;}
.product_detail_table table{border-collapse:collapse;border-spacing:0;}
.product_detail_param{line-height:30px;overflow:auto}
.product_detail_param table{border-collapse:collapse;border-spacing:0;}
.product_detail_param th{background: none repeat scroll 0 0 #F9F9F9;}
.product_detail_param td{background: none repeat scroll 0 0 #FFFFFF;}
.product_detail_param td,.product_detail_param th{border: 1px solid #F5F5F5;padding:5px;}
.product_detail_param td strong{background: none repeat scroll 0 0 #E8E6E7;display: block;line-height: 30px;padding: 0 15px;}
.product_detail_param .td_head{padding:0;}
/*产品咨询*/
.consult_list{padding:0 15px 15px}
.consult_list li{margin-bottom:10px;background-color:#fff}
.consult_list .ui-icon-arrow-r{color:#c9c9c9}
.consult_list dl,.consult_detail dl{margin:0;font-size:90%;padding:4px 0;line-height:150%}
.consult_list dt,.consult_detail dt{float:left;width:75px;text-align:right;}
.consult_list dd,.consult_detail dd{margin-left:75px;padding-right:15px;white-space:normal; text-overflow:ellipsis}
.consult_detail{padding:10px;background-color:#fff}
.dashline{margin:10px 0;border-bottom:1px dashed #c9c9c9}
/*收藏夹*/
.favorite_index{padding-top:20px;text-align:center;color:#c9c9c9}
.favorite_index h4{margin-bottom:10px;font-size:80px}
.favorite_index p{font-size:100%;line-height:160%}
.favorite_list{padding:15px}
.favorite_list li{background-color:#fff; border-bottom:1px dotted #c9c9c9}
.favorite_list .ui-li-thumb{top:22px}
.favorite_list h5{font-size:80%}
.favorite_list .ui-icon-arrow-r:before{content:""}
.favorite_list .del{position:absolute;right:20px;top:50%;margin-top:-10px;line-height:20px;color:#fd6566}
/*收货地址*/
.address{padding:15px}
.address li{margin-bottom:10px;background-color:#fff}
.address h3{margin-bottom:15px;padding-bottom:10px;border-bottom:1px dotted #cacaca;font-size:100%;overflow:hidden;zoom:1}
.address h3 span{float:right}
.address p{font-size:100%;line-height:160%}
.address p span{padding-left:40px}
/*投诉与建议*/
.feedback_index{padding-top:20px;text-align:center;color:#c9c9c9}
.feedback_index h4{margin-bottom:10px;font-size:80px}
.feedback_index p{font-size:100%;line-height:160%}
.feedback_list{padding:15px}
.feedback_list li{margin-bottom:10px;background-color:#fff}
.feedback_list dl,.feedback_detail dl{margin:0;font-size:90%;padding:4px 0;line-height:150%}
.feedback_list dt,.feedback_detail dt{float:left;width:75px;text-align:right;}
.feedback_list dd,.feedback_detail dd{margin-left:75px;padding-right:15px;white-space:normal; text-overflow:ellipsis}
.feedback_detail{padding:10px;background-color:#fff}
.feedback_img em{float:left;width:50px;overflow:hidden}
.feedback_img img{margin-left:-15px;height:60px}
.feedback_form{margin-bottom:15px;padding:10px;background-color:#fff}
.feedback_form .p_txt{margin-top:10px;padding:2px 5px;background-color:#f4f4f4}
.feedback_form .p_txt input,.feedback_form .p_txt textarea{border:0;width:100%;background:none}
.feedback_form .p_txt textarea{height:100px}
.feedback_form .f_type{padding-bottom:10px;border-bottom:1px dotted #c9c9c9}
.feedback_form dt{width:100px;float:left;line-height:40px}
.feedback_form dd{margin-left:80px}
.upload_box{padding:10px 0;overflow:hidden;zoom:1}
.upload_box p{float:left;width:80px;height:80px;border:1px dotted #b5b5b5;text-align:center;line-height:80px;font-size:40px;color:#d4d4d4}
.upload_box span{display:block;margin-left:100px;line-height:80px;font-size:80%}